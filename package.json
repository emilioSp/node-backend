    "@koa/router": "^10.1.1",
    "ajv": "^8.11.0",
    "better-ajv-errors": "^1.2.0",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-no-slash": "^1.0.2",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.4",
